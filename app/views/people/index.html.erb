

<h1>People</h1>
<%= Time.current  %>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>


    <% @peoples.each do |people| %>
      <tr>
        <td><%= people.name %></td>
        <td><%= people.city %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New', "/people/new" %>
